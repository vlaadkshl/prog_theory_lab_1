<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="1" author="vlaadkshl">
        <createTable tableName="create_data">
            <column name="id" type="BIGINT">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="table_name" type="VARCHAR(100)">
                <constraints nullable="false"/>
            </column>
            <column name="data_json" type="text">
                <constraints nullable="false"/>
            </column>
            <column name="queried_at" type="timestamp">
                <constraints nullable="false"/>
            </column>
        </createTable>
        <addAutoIncrement tableName="create_data" columnName="id"
                          generationType="ALWAYS"
                          incrementBy="1"
                          columnDataType="BIGINT"/>
    </changeSet>
</databaseChangeLog>